{
  "presets": [
    "flow",
    ["env", {
      "targets": {
        "node": true
      }
    }]
  ],
  "plugins": [
    [
      "babel-plugin-transform-builtin-extend",
      { "globals": [ "Error", "Array" ] }
    ],
    "transform-object-rest-spread"
  ],
  "sourceMaps": "both",
  "retainLines": true
}